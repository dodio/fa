{   
    // 服务器本身配置
    "fa": {
        "port": 80, //运行端口
        "view": {
            "ext": "tpl"
        }
    },
    // 模板引擎配置
    "swig": {
        "cache": false,
        "autoescape": true,
        "varControls": ["{{", "}}"],
        "tagControls": ["{%", "%}"],
        "cmtControls": ["{#", "#}"]
    },
    "middleware": {
        "bodyParser": {
            "level": 1,
            "urlencoded": {
                "extended": false
            }
        },
        "cookieParser": {
            "level": 4,
            "options": {
                "secret": "zmskUKsl*^%"
            }
        },
        "horse":{
            "disable": true,
            "level": 5
        }
    }
    
}